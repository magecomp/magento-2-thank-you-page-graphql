        pageSize: Int = 5 @doc(description: "Number of items should be shown on the page"),
        currentPage: Int = 1 @doc(description: "Allow page number to start from 1")
    ):MageplazaThankYouPageOutput @resolver(class: "Mageplaza\\ThankYouPageGraphQl\\Model\\Resolver\\ThankYouPage") @doc(description: "Queries search for Mageplaza Thank You Page that match the filtered criteria.")
}

input MageplazaThankYouPageFilterInput {
    template_id: FilterTypeInput @doc(description: "Template Id")
    name: FilterTypeInput @doc(description: "Name")
    page_type: FilterTypeInput @doc(description: "Page Type")
    status: FilterTypeInput @doc(description: "Status")
    priority: FilterTypeInput @doc(description: "Priority")
